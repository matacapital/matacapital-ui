{
	"compilerOptions": {
		"jsx": "react",
		"target": "ESNext",
		"module": "CommonJS",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"noUncheckedIndexedAccess": true,
		"allowImportingTsExtensions": true
	}
}
